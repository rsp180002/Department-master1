<%- include('_header'); %>

<section class="mt-3 col-12">
  <h2>Avarage Duration in a Title</h2>
  <% formatedTime.forEach(function(each){ %>
  <div class="card">
    <div class="card-body">
      <h5>
        <%= each._id %> : <%= each.time.year %> Year <%= each.time.month %>
        Month <%= each.time.day %> Day
      </h5>
    </div>
  </div>
  <% }) %>
</section>
<%- include('_footer'); %>
